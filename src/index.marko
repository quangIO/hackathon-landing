    hero-card.section--contact
      @title
        div.wow.fadeInUp
          b#contact-us -- Liên hệ
      @content
        div.container.wow.fadeIn
          div.columns
            div.column.has-text-centered
              form name="contact" method="POST" netlify
                p
                  input type="text" name="name" placeholder="Tên của bạn"
                p
                  input type="email" name="email" placeholder="Email"
                p
                  textarea name="message" placeholder="Nội dung tin nhắn"
                div data-netlify-recaptcha
                p
                  button.gbutton name="submit" type="submit" -- Gửi
            div.column.is-pulled-right-desktop.has-text-centered-mobile
              p
                a href="mailto:team@shecodes.tech" -- Email Us
              p
                a href="#" -- Facebook Page
